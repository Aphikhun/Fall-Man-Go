#version 100

uniform sampler2D texSampler;

varying mediump vec2 texCoord;

void main(void)
{
	mediump vec4 textureColor = texture2D(texSampler, texCoord);
	mediump vec4 weight = vec4(0.4, 0.4, 0.4 , 1);
	if (textureColor.w < 0.11)
		gl_FragColor = vec4(1.0, 1.0, 1.0, 0);
	else
		gl_FragColor = textureColor  * weight;
}