#version 300 es

precision mediump float;

#include "headers/defer_basic.glsl"
//#include "headers/lighting.glsl"
//#include "headers/shadow.glsl"
//#include "headers/fog.glsl"

uniform sampler2D texSampler;
uniform float discardAlpha;


void main(void)
{
	vec4 diffuse = texture(texSampler, texCoord);
	if (diffuse.a < discardAlpha)
	{
		discard;
	}

	DrawToGBuffers(diffuse);
}
