#version 100

precision mediump float;

uniform sampler2D texSamplerModel;
uniform sampler2D texSamplerEye;
uniform sampler2D texSamplerMouse;
uniform sampler2D texSamplerCosmetics;
uniform sampler2D texSamplerTattoo;

varying mediump vec2 texCoord;

void main(void)
{
	vec4 finalColor = texture2D(texSamplerEye, texCoord);
	if(finalColor.a < 0.1)
		finalColor = texture2D(texSamplerMouse, texCoord);
	if(finalColor.a < 0.1)
		finalColor = texture2D(texSamplerCosmetics, texCoord);
	if(finalColor.a < 0.1)
		finalColor = texture2D(texSamplerTattoo, texCoord);
	if(finalColor.a < 0.1)
		finalColor = texture2D(texSamplerModel, texCoord);
	
	gl_FragColor = finalColor;
}