ENTITY_TOUCH_ALL {
	If (not IsValueInArray(array = $obj1.listPlayer, value = $obj2, entity = $obj1)) {
		ArrayAppend(
			array = $obj1.listPlayer,
			value = $obj2,
			entity = $obj1
		)
		$obj2.NPC_Start = $obj1
	}
	ShowTip(
		entity = $obj2,
		keepTime = 200,
		textKey = ToString(ArraySize(array = $obj1.listPlayer, entity = $obj1)),
		tipType = "1"
	)
}
