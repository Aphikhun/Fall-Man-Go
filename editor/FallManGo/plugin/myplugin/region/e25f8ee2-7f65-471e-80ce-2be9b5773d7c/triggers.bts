REGION_ENTER {
	$obj1.EnterGoal = true
	$num = NumberFloor(p1 = 0)
	Foreach(array = @List, key = "i") {
		$obj1.listPlayer = $i
		If (IsValueInArray(array = $obj1.listPlayer, value = $obj1, entity = $obj1)) {
			Foreach(array = $obj1.listPlayer, key = "j") {
				If ($j.EnterGoal) {
					$num = $num + 1
				}
			}
		}
		If ($num >= 2) {
			$map = CreateDuplication(name = "4bf28a4f-8de2-427c-8b8c-c87215843a0b")
			ShowTip(entity = $obj1, keepTime = 200, textKey = "38551935-6317-4856-a1c1-71c7b51c4c50", tipType = "1")
			Foreach(array = $obj1.listPlayer, key = "player") {
								If ($player.EnterGoal) {
					EnterDuplication(entity = $player, map = $map, pos = { x = 23, y = 53, z = 27 }, rp = 0, ry = 0)
				} ElseIf (not $player.EnterGoal) {
					EnterMap(entity = $player, map = "map001", pos = GetEntityStartPos(entity = $player), rp = 0, ry = 0)
					ArrayRemove(
						array = $obj1.listPlayer,
						index = ArrayFind(
						array = $obj1.listPlayer,
						value = $player,
						entity = $obj1
					),
						entity = $obj1
					)
				}
				If ($player.EnterGoal) {
					$player.EnterGoal = false
				}
			}
		}
	}
}
